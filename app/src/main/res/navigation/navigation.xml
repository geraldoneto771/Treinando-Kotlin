<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/intro">

    <fragment
        android:id="@+id/intro"
        android:name="com.example.apptreinando.Intro"
        android:label="fragment_intro"
        tools:layout="@layout/fragment_intro" >
        <action
            android:id="@+id/action_intro_to_home"
            app:destination="@id/home" />
    </fragment>
    <fragment
        android:id="@+id/home"
        android:name="com.example.apptreinando.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_registre"
            app:destination="@id/registre" />
    </fragment>
    <fragment
        android:id="@+id/registre"
        android:name="com.example.apptreinando.Registre"
        android:label="fragment_registre"
        tools:layout="@layout/fragment_registre" />
</navigation>